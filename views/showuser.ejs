<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SHOW ALL USERS</title>
</head>
<style>
    table,th,td,tr{
        border: 1px solid black;
    }
</style>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
        
        
        
             <% for(let user of users){ %>

                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                    <td>
                        <form action="/user/<%= user.id %>/edit"  method="get">
                            <button>Edit username</button>
                        </form>
                    </td>
                </tr>
            
            
             <% } %>
        
    
    </table>
</body>
</html>




